{
  "name": "keyboard-tracker",
  "version": "1.0.1",
  "description": "keyboard tracker for stream / video overlays",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/KlassyKat/Keyboard-Tracker.git"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.keybord.tracker",
    "productName": "Keyboard Tracker",
    "win": {
      "target": "nsis",
      "icon": "./buildResources/iconv2.ico",
      "oneClick": false,
      "publish": "github"
    }
  },
  "author": "GabeAlexander",
  "license": "ISC",
  "dependencies": {
    "@simonwep/pickr": "^1.5.1",
    "electron-window-state": "^5.0.3",
    "font-list": "^1.2.6",
    "iohook": "^0.6.5",
    "mouse-position": "^2.1.0",
    "node-localstorage": "^2.1.5"
  },
  "devDependencies": {
    "electron": "^8.0.1",
    "browser-sync": "^2.26.7",
    "node-abi": "^2.15.0"
  },
  "iohook": {
    "targets": [
      "node-79",
      "electron-76"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
